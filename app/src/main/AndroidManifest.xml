<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="io.github.ketcktsd.anktfw">

    <uses-permission android:name="android.permission.INTERNET" />
    <application
        android:name=".MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:ignore="AllowBackup,GoogleAppIndexingWarning">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".androidarch.AsyncSampleActivity"
            android:label="@string/title_arch_sample" />
        <activity
            android:name=".di.DISampleActivity"
            android:label="@string/title_di_sample" />
        <activity
            android:name=".animation.AnimationSampleActivity"
            android:label="@string/title_animation_sample" />

        <activity android:name=".bind.BindSampleActivity"
            android:label="@string/title_bind_sample"/>
    </application>

</manifest>
